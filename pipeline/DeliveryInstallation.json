{
	"name": "DeliveryInstallation",
	"properties": {
		"activities": [
			{
				"name": "Copy_Input_Staging",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "input/int_19/del_inst/"
					},
					{
						"name": "Destination",
						"value": "staging/del_inst/"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFolderPath": "int_19/del_inst",
							"wildcardFileName": "*"
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DS_Metadata_Del_Inst",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_DL_INST_NEW",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}